{
  "name": "backend-app-meli",
  "version": "0.3.0",
  "scripts": {
    "dev": "node --inspect=5858 -r ts-node/register src/bin/www.ts",
    "start": "node build/server/bin/www.js",
    "build": "tsc",
    "release": "standard-version",
    "watch": "nodemon",
    "test": "jest",
    "coveralls": "cat ./coverage/lcov.info | node node_modules/.bin/coveralls",
    "performance": "node scripts/autocannon.js"
  },
  "nodemonConfig": {
    "ignore": [
      "**/*.test.ts",
      "**/*.spec.ts",
      ".git",
      "node_modules"
    ],
    "watch": [
      "src"
    ],
    "exec": "npm dev",
    "ext": "ts js"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "author": "mtorre4580@outlook.com",
  "license": "ISC",
  "dependencies": {
    "axios": "0.18.0",
    "body-parser": "1.18.3",
    "compression": "1.7.4",
    "cors": "2.8.5",
    "dnscache": "^1.0.1",
    "express": "4.16.4",
    "express-healthcheck": "^0.1.0",
    "method-override": "3.0.0",
    "newrelic": "5.6.4",
    "sanitize-html": "1.20.0",
    "winston": "3.2.1",
    "swagger-ui-express": "4.0.2"
  },
  "devDependencies": {
    "@commitlint/cli": "7.5.2",
    "@commitlint/config-conventional": "7.5.0",
    "@types/body-parser": "1.17.0",
    "@types/compression": "0.0.36",
    "@types/cors": "2.8.4",
    "@types/express": "4.16.1",
    "@types/http-status": "0.2.30",
    "@types/jest": "24.0.11",
    "@types/method-override": "0.0.31",
    "@types/newrelic": "4.11.0",
    "@types/node": "11.11.6",
    "@types/supertest": "2.0.7",
    "@types/swagger-ui-express": "3.0.0",
    "autocannon": "3.2.1",
    "coveralls": "3.0.3",
    "eslint-plugin-import": "2.17.2",
    "eslint": "5.16.0",
    "husky": "1.3.1",
    "jest": "22.4.4",
    "nodemon": "1.18.11",
    "standard-version": "5.0.2",
    "supertest": "3.4.2",
    "ts-jest": "22.4.6",
    "ts-node": "8.1.0",
    "tslint": "5.14.0",
    "typescript": "3.4.4"
  },
  "engines": {
    "node": ">=8.9.4 <10.0.0 || >=10.4.0"
  }
}
